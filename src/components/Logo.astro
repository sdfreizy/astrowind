---
import { SITE } from 'astrowind:config';
import Image from '~/components/common/Image.astro';
import logo from '~/assets/images/rmlogonav.svg';
---

<Image
  src={logo}
  class="h-8 lg:h-12 object-contain"
  widths={[52, 77]}
  width={52}
  sizes="(max-width: 1023px) 52px, 77px"
  alt={SITE?.name}
  aspectRatio="16:10"
  loading="lazy"
  decoding="async"
/>
